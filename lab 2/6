#include <iostream>

using namespace std;

int main()

{
    int a = 1, i = 0, max = -999999;
    while (true)
    {
        cin >> a;
        if (a == 0)
            break;
        if (a > max)
        {
            max = a;
            i = 0;
        }
        if (a == max)
            i++;
    }
    cout << max << ' ' << i;
    return 0;
}


