#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main()
{
    int n, p = 0;
    cin >> n;
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < n - p; j ++)
            cout << "*";
        for (int j = 0; j < p; j ++)
            cout << " ";
        cout << endl;
        p++;
    }
    return 0;
}
